{
  "name": "builder-factory",
  "version": "2.1.4",
  "description": "Factory for creating basic object types during testing.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm-run-all build:types build:js",
    "build:js": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "test": "npm run build && jest",
    "shipit": "npm run test && npm version patch && npm publish"
  },
  "author": "Kevin Wilson <kevin@partcoffee.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kwilson/builder-factory"
  },
  "files": [
    "dist",
    "index.d.ts"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.138",
    "babel": "^6.23.0",
    "jest": "^24.9.0",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.20.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3"
  },
  "dependencies": {},
  "jest": {
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.test.json"
      }
    }
  }
}
